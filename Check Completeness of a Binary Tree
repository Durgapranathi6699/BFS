class Solution {
    public boolean isCompleteTree(TreeNode root) {
      Queue<TreeNode> q=new LinkedList<>();
      q.add(root);
      boolean flag=false;
      while(!q.isEmpty()){
          TreeNode t=q.poll();
          if(t==null)flag=true;
          else{
              q.add(t.left);
              q.add(t.right);
              if(flag){
                  return false;
              }
          }
      }
      return true;
    }
   
}
